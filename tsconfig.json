{
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["vite/client"],
    "resolveJsonModule": true,
    "esModuleInterop": true,

    "target": "ES2020",
    "lib": ["DOM", "ES2020", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "strict": true
  },

  "include": ["src"]
}
